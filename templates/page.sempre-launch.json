        "9a5aef13-6981-4512-bb36-b707bab54b3b": {
          "type": "text_block",
          "settings": {
            "reverse": false,
            "image": "shopify:\/\/shop_images\/IMG_3044-2.jpg",
            "text": "<h2>ORGANONUTRIENTS<\/h2><p>ALL NATURAL BOTANICALS<\/p><p>ORGANONUTRIENTS are a proprietary formulation of natural botanicals, scientifically enhanced nutrients and bioactives specially selected by physicians for their active properties and benefits to human hair and skin.<\/p>",
            "background_color": "#000000",
