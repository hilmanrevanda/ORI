    "fh_section_richtext_4fYhEe": {
      "type": "fh-section-richtext",
      "settings": {
        "text": "<h3>Congratulations on taking the first step towards fuller, healthier hair with Sempre All-Natural Hair Growth Foam! Your hair growth journey is like crafting a masterpiece — time and patience are key.<\/h3>",
        "background_color": "#000000",
        "text_color": "#ffffff",
        "image": "shopify:\/\/shop_images\/Sempre_Quotes_Testimonials_1.png"
      }
    },
    "e8e433ee-440d-4d90-afb3-b82387ee8444": {
      "type": "fh-section-custom-list-richtext",
      "blocks": {
        "9a5aef13-6981-4512-bb36-b707bab54b3b": {
          "type": "text_block",
          "settings": {
            "reverse": false,
            "image": "shopify:\/\/shop_images\/IMG_3417-3.jpg",
            "text": "<h2>SEMPRE ORGANONUTRIENTS<\/h2><p>ALL NATURAL BOTANICALS<\/p><p>ORGANONUTRIENTS are a proprietary formulation of natural botanicals, scientifically enhanced nutrients and bioactives specially selected by physicians for their active properties and benefits to human hair and skin.<\/p>",
            "background_color": "#000000",
            "text_color": "#ffffff"
          }
        }
      },
      "block_order": [
        "9a5aef13-6981-4512-bb36-b707bab54b3b"
